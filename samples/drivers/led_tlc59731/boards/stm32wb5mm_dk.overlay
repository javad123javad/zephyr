/ {

chip-select {
	compatible = "gpio-leds";

	rgb_cs: rgb_cs {
		gpios = <&gpioh 1 (GPIO_PULL_DOWN | GPIO_ACTIVE_HIGH)>;
		label = "RGB_LED_CS";
	};
};

aliases {
	rgb-led-cs = &rgb_cs;
};

};
